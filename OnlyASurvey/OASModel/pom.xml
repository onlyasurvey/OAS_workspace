		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-core</artifactId>
			<version>2.5.6</version>
		</dependency>
		<dependency>
			<groupId>ca.inforealm.core</groupId>
			<artifactId>SaneStackJAR</artifactId>
			<version>0.1</version>
		</dependency>

		<!-- For reporting. -->
		<dependency>
			<groupId>org.apache.poi</groupId>

		<groupId>com.oas</groupId>
		<version>1.0-SNAPSHOT</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.oas</groupId>
	<artifactId>OASModel</artifactId>
	<name>OnlyASurvey.com Model</name>
	<version>1.0-SNAPSHOT</version>
	<url>http://www.onlyasurvey.com</url>


	<build>
		<plugins>
